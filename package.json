{
  "name": "ayonne",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "node .next/standalone/server.js",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "validate": "npx ts-node scripts/validate.ts",
    "validate:full": "npx ts-node scripts/validate.ts --build",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^6.8.0",
    "@stripe/stripe-js": "^8.6.0",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "prisma": "^6.8.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "replicate": "^1.4.0",
    "stripe": "^20.1.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@shopify/cli": "^3.88.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.27",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
